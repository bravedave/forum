{
  "name": "bravedave/forum",
  "description": "FORUM module for bravedave/DVC",
  "keywords": [
    "Forum"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Bray",
      "email": "david@brayworth.com.au"
    }
  ],
  "require": {
    "bravedave/dvc": "*",
    "bravedave/green": "*"
  },
  "require-dev": {
    "matthiasmullie/scrapbook": "*"
  },
  "autoload": {
    "psr-4": {
      "dvc\\forum\\": "src/forum/",
      "dvc\\idea\\": "src/idea/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "src/app/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "dvc\\forum\\postUpdate::upgrade"
    ],
    "start": "bash ./run.sh"
  }
}